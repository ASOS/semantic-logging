<?xml version="1.0"?>
<configuration xmlns="http://schemas.microsoft.com/practices/2013/entlib/semanticlogging/etw">
  <traceEventService />
  <sinks>
    <flatFileSink fileName="FlatFileXmlFormatterDateTimeFormat.log" name="FlatFileEventListener">
      <sources>
        <eventSource name="MockEventSourceOutProc" />
      </sources>
      <xmlEventTextFormatter formatting="Indented" dateTimeFormat="dMyyyy" />
    </flatFileSink>
  </sinks>

</configuration>