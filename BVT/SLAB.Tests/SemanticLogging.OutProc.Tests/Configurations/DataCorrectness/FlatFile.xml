<?xml version="1.0"?>
<configuration xmlns="http://schemas.microsoft.com/practices/2013/entlib/semanticlogging/etw">
  <sinks>
    <flatFileSink  fileName=".\Logs\OutProcFlatFileData.log" name="FlatFileEventListener">
      <sources>
        <eventSource name="replaceEventSource"/>
      </sources>
      <eventTextFormatter dateTimeFormat="dMyyyy" verbosityThreshold="LogAlways" />
    </flatFileSink>
  </sinks>
</configuration>
